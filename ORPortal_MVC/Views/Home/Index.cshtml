@{
    ViewBag.Title = "Index";
}
<script type="text/javascript">

    $(function () {
        $("#TextBox1").text();
        $("#TextBox2").text();
        $("#CP_Setting")
            .button()
            .css('background', '#FF9B6A')
            .css('color', 'black')
            .css('border', '2px solid #CC6666')
            .css('font-weight', 'normal')
            .css('font-size', 'medium')
            .click()
        .mouseover(function () {
            $(this).css('border','2px solid #993300')
        })
        .mouseout(function () {
            $(this).css('border', '2px solid #CC6666')
        });
        $("#CP_Display")
            .button()
            .css('background', '#FF9B6A')
            .css('color', 'black')
            .css('border', '2px solid #CC6666')
            .css('font-weight', 'normal')
            .css('font-size', 'medium')
            .click()
        .mouseover(function () {
            $(this).css('border', '2px solid #993300')
        })
        .mouseout(function () {
            $(this).css('border', '2px solid #CC6666')
        });


    });
    //function Dark(mnu) {

    //    document.getElementById(mnu).style.border = '2px solid #993300';
    //}
    //function Light(mnu) {

    //    document.getElementById(mnu).style.border = '2px solid #CC6666';
    //}

</script>
